# Frontend Environment Variables
# Copy this file to .env and modify the values as needed

# Backend API Base URL (for production)
VITE_API_BASE=

# Development Backend Server (modify if your backend runs on different host/port)
VITE_DEV_BACKEND_HOST=127.0.0.1
VITE_DEV_BACKEND_PORT=8000

# OpenWeather API Key (optional, for weather features)
VITE_OPENWEATHER_API_KEY=